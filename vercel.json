{
  "version": 2,
  "rewrites": [
    { "source": "/api/webhook", "destination": "/api/webhook.js" },
    { "source": "/music/(.*)", "destination": "/music/$1" }
  ],
  "headers": [
    {
      "source": "/music/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "audio/mpeg" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
